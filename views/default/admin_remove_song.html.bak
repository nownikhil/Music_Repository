{{extend 'layout.html'}}
<b><font size="5">List of Songs</b>
<br></br>
<table cellspacing="6">
{{for song in songs:}}
    <br><tr>
    <td width="30%"><font size="3">{{=song.Name}}</font>
    <td width="30%"><font size="3">{{=song.artist}}</font>
    <td width="20%"><font size="3">{{=song.album}}</font>
    <td width="30%"><font size="2">{{=A('Delete Song',_href=URL(r=request,f="delete_song",args=song.id))}}</font></td></tr></br>

    
{{pass}}
</table>
